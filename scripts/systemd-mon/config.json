{
	"apache2": {
		"manual": 1,
		"pid": {
			"triggers": {
				"trig1": {
					"prior": "warn",
					"descr": "SUPP: Service probably isn't working.",
					"range": [-1, -1],
					"resol": "Restart apache2 via systemctl."
				}
			}
		},
		"io_write_usage": {
			"triggers": {
				"trig1": {
					"prior": "err",
					"descr": "SUPP: Apache2 is writing too much on disk.",
					"range": [90, 100],
					"resol": "Don't worry, be happy"
				},
				"trig2": {
					"prior": "warn",
					"descr": "SUPP: Apache2 is writing a lot on disk.",
					"range": [80, 90],
					"resol": "Be nicer to apache"
				}
			}
		},
		"io_read_usage": {
			"triggers": {
				"trig1": {
					"prior": "err",
					"descr": "SUPP: Apache2 is reading too much on disk.",
					"range": [90, 100],
					"resol": "Don't worry, be happy"
				},
				"trig2": {
					"prior": "warn",
					"descr": "SUPP: Apache2 is reading a lot on disk.",
					"range": [80, 90],
					"resol": "Be nicer to apache"
				}
			}
		},
		"status_service": {
			"triggers": {
				"trig1": {
					"prior": "info",
					"descr": "SUPP: Apache2 is disabled!",
					"range": [0, 0],
					"resol": "Idk"
				}
			}
		}
	},
